{
  "name": "boilerplate-app",
  "version": "0.0.0",
  "scripts": {
    "build": "next build && next export -o build-next-static",
    "dev": "open http://localhost:3100 && next dev --port 3100",
    "lint": "eslint --max-warnings 0 .",
    "lint-fix": "eslint --fix .",
    "tsc": "tsc -p tsconfig.json",
    "test": "jest --verbose",
    "test-watch": "jest --verbose --watch",
    "test-coverage": "jest --coverage --verbose",
    "storybook": "start-storybook -p 6100",
    "storybook-build": "build-storybook -o build-storybook-static",
    "update-dependencies": "npx npm-check-updates -u",
    "api-generate-petstore": "ts-node-esm -P tsconfig.openapi-typescript.json ./src/common/api/petstore/api-types/generate/index.mts"
  },
  "dependencies": {
    "config-mui": "*",
    "react-parallax-tilt": "1.7.67",
    "ui": "*",
    "utils-mui": "*"
  },
  "devDependencies": {
    "chalk": "5.0.1",
    "config-tsconfig": "*",
    "openapi-typescript": "5.4.1",
    "utils-test": "*"
  }
}
